{
  "name": "ext-react",
  "version": "1.7.0",
  "description": "JavaScript library that build on top of React",
  "main": "src/rext.js",
  "author": "Lionel Nguyen <huynguyen8505@gmail.com> (https://github.com/huytrongnguyen)",
  "license": "MIT",
  "keywords": [
    "extension",
    "react"
  ],
  "homepage": "https://huytrongnguyen.github.io/extension-react",
  "repository": {
    "type": "git",
    "url": "https://github.com/huytrongnguyen/extension-react.git"
  },
  "bugs": "https://github.com/huytrongnguyen/extension-react/issues",
  "scripts": {
    "style": "node-sass --include-path scss css/rext.scss src/rext.min.css --output-style compressed && gulp copy",
    "server": "babel-node server.js --presets env"
  },
  "files": [
    "LICENSE",
    "src/"
  ],
  "babel": {
    "presets": [ "env", "react" ],
    "plugins": [
      "transform-decorators-legacy",
      "transform-function-bind",
      "transform-object-rest-spread",
      ["babel-plugin-root-import", [{
          "rootPathSuffix": "src/"
        }, {
          "rootPathPrefix": "@",
          "rootPathSuffix": "docs/src/js/"
        }]
      ]
    ],
    "ignore": "node_modules"
  },
  "devDependencies": {
    "babel-polyfill": "^6",
    "react": "^15",
    "react-dom": "^15",
    "font-awesome": "^4",
    "d3":"^4",
    "babel-cli": "6.24.1",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-function-bind": "6.22.0",
    "babel-plugin-transform-object-rest-spread": "6.23.0",
    "babel-plugin-root-import": "5.1.0",
    "babel-preset-env":"1.6.0",
    "babel-preset-react": "6.24.1",
    "babelify": "7.3.0",
    "browserify": "14.4.0",
    "express": "4.15.3",
    "gulp": "3.9.1",
    "gulp-sass": "3.1.0",
    "gulp-streamify": "1.0.2",
    "gulp-uglify": "3.0.0",
    "jest": "20.0.4",
    "node-sass": "4.5.3",
    "vinyl-source-stream": "1.1.0",
    "watchify":"3.9.0",
    "yarn": "0.27.5"
  }
}
